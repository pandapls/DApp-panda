#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules/backstopjs/cli/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules/backstopjs/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules/backstopjs/cli/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules/backstopjs/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/backstopjs@6.3.23_typescript@5.4.5/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../backstopjs/cli/index.js" "$@"
else
  exec node  "$basedir/../backstopjs/cli/index.js" "$@"
fi
