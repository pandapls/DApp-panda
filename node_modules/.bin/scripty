#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/scripty@2.1.1/node_modules/scripty/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/scripty@2.1.1/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/scripty@2.1.1/node_modules/scripty/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/scripty@2.1.1/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../scripty/cli.js" "$@"
else
  exec node  "$basedir/../scripty/cli.js" "$@"
fi
