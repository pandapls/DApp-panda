#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules/jest-stare/lib/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules/jest-stare/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules/jest-stare/lib/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules/jest-stare/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/jest-stare@2.5.1_@popperjs+core@2.11.8_node-notifier@10.0.1/node_modules:/Users/lhf/Desktop/repo/dapp-panda/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest-stare/lib/jest-stare.js" "$@"
else
  exec node  "$basedir/../jest-stare/lib/jest-stare.js" "$@"
fi
